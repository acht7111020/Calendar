<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeanlee.calendar" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".JournalActivity"
            android:label="@string/title_activity_journal" >
        </activity>
        <activity
            android:name=".AlbumActivity"
            android:label="@string/title_activity_album" >
        </activity>
        <activity
            android:name=".TodoActivity"
            android:label="@string/title_activity_todo" >
        </activity>
        <activity
            android:name=".CalendarActivity"
            android:label="@string/title_activity_calendar" >
        </activity>
        <activity
            android:name=".NoteActivity"
            android:label="@string/title_activity_note" >
        </activity>
        <activity
            android:name=".JournalActivity_card"
            android:label="@string/title_activity_journal_activity_card" >
        </activity>
        <activity
            android:name=".CalendarViewActivity"
            android:label="@string/title_activity_calendar_view" >
        </activity>
    </application>

</manifest>
